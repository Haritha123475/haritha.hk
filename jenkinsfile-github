pipeline {
    agent any

    stages {
        
        stage('build') {
            steps {
                echo 'build is successfully using maven'
            }
        }
        stage('test') {
            steps {
                echo 'the test cases will be executed sucessfully'
            }
        }
        stage('deploy') {
            steps {
                echo 'deployment is successfully completed'
            }
        }
    }
}
